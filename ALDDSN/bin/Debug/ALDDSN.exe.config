  <appSettings>
    
    <add key="ddtype" value="0" />
    <!--区域-->
    <add key="regionId" value="cn-shenzhen" />
    <!--阿里云accessKeyId-->
    <add key="accessKeyId" value="LTAI4FenH8UPdpqbZcA5buDo" />
    <!--阿里云accessKeySecret-->
    <add key="accessKeySecret" value="iYmmcxpX5c1Pl7IpDozuL4dW7vyqnj" />
